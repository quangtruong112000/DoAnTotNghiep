
@{
    ViewBag.Title = "LayoutShare";
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home</title>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700;900&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css" />
    <link rel="stylesheet"
          href="https://unpkg.com/swiper/swiper-bundle.min.css" />
    <link href="~/Areas/Customer/css/home.css" rel="stylesheet" />
    <link href="~/Areas/Customer/css/services.css" rel="stylesheet" />
    <link href="~/Areas/Customer/css/comestic.css" rel="stylesheet" />
    <link href="~/Areas/Customer/css/layout.css" rel="stylesheet" />
    <link href="~/Areas/Customer/css/doctor.css" rel="stylesheet" />
    <link href="~/Areas/Customer/css/doctor-intro.css" rel="stylesheet" />
    <link href="~/Areas/Customer/css/cosmetics-detail.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Rubik:wght@400;500;700&display=swap" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700;900&display=swap" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.5.0.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.1/css/all.min.css" />
    <link href="~/Areas/Customer/css/layout.css" rel="stylesheet" />
    <link href="~/Content/PagedList.css" rel="stylesheet" />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
            crossorigin="anonymous"></script>
</head>

<body>
    <div class="header">
        <div class="header-container">
            <div class="header-left">
                <a href="/home.html" class="header-logo-content">
                    <img src="https://online.vietskin.vn/logo-brand.svg" alt="logo-header">
                    <div class="header-branch">DrugStore</div>
                </a>
            </div>
            <div class="header-menu">
                <ul class="list-item">
                    <li class="menu-item">TRANG CHỦ</li>
                    <li class="menu-item"><a href="/Customer/Service/Index">DỊCH VỤ</a> </li>
                    <li class="menu-item" ><a href="/Customer/Comestic/Index">MỸ PHẨM</a> </li>
                    <li class="menu-item">BÁC SĨ</li>
                    <li class="menu-item">PHIÊN KHÁM CỦA TÔI</li>
                    <li class="menu-item">DIỄN ĐÀN</li>
                </ul>
            </div>
            <div class="header-right">
                <a href="/login.html" class="header-account">Đăng nhập / Đăng kí</a>
            </div>
        </div>
    </div>
    <div>
     
        @{Html.RenderAction("Cart", "Comestic");};
        @RenderBody()
    </div>
    <div class="footer-main">
        <div class="container">
            <div class="footer-main-branch">
                <img src="https://online.vietskin.vn/Logo.png" alt="footer-logo">
            </div>
            <div class="footer-main-list">
                <div class="footer-main-list-title">
                    Dịch vụ
                </div>
                <ul class="footer-main-list-item">
                    <li class="item">
                        <a href="">Spa</a>
                    </li>
                    <li class="item">
                        <a href="">Chuẩn đoán</a>
                    </li>
                    <li class="item">
                        <a href="">Khám bệnh</a>
                    </li>
                </ul>
            </div>
            <div class="footer-main-list">
                <div class="footer-main-list-title">
                    Cửa hàng
                </div>
                <ul class="footer-main-list-item">
                    <li class="item">
                        <a href="">Dược phẩm</a>
                    </li>
                    <li class="item">
                        <a href="">Mỹ phẩm</a>
                    </li>
                </ul>
            </div>
            <div class="footer-main-list">
                <div class="footer-main-list-title">
                    Hồ sơ cá nhân
                </div>
                <ul class="footer-main-list-item">
                    <li class="item">
                        <a href="">Lịch sử phiên khám</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>

    <script>
        var swiper = new Swiper(".mySwiper", {});
    </script>
    <script type="text/javascript" src="https://code.jquery.com/jquery-1.11.0.min.js"></script>
    <script type="text/javascript" src="https://code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>
    <script src="~/Areas/Scripts/cosmetics-detail.js"></script>
    <script src="~/Areas/Scripts/cosmetics.js"></script>
</body>

</html>
